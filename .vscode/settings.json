{
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "typescript.tsdk": "frontend/node_modules/typescript/lib",
    "editor.rulers": [100],
    "files.exclude": {
        "node_modules": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/.git": true,
        "frontend/dist": true
    }
}
