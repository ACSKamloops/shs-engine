{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:smoke": "playwright test tests/smoke.spec.ts",
    "test:smoke:preview": "bash -c 'npm run preview -- --host --port 4173 >/dev/null 2>&1 & PREVIEW_PID=$!; PLAYWRIGHT_BASE_URL=http://localhost:4173 npm run test:smoke; kill $PREVIEW_PID'",
    "test:smoke:live": "PLAYWRIGHT_BASE_URL=${PLAYWRIGHT_BASE_URL:-http://localhost:8000} playwright test tests/smoke.spec.ts",
    "test:smoke:api": "npm run build && PLAYWRIGHT_BASE_URL=${PLAYWRIGHT_BASE_URL:-http://localhost:8000} playwright test tests/smoke.spec.ts",
    "test:unit": "vitest",
    "test": "npm run lint && npm run test:unit && npm run test:smoke:preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN --storybook-build-dir=storybook-static",
    "test:e2e:serve": "bash ./scripts/run_e2e.sh"
  },
  "dependencies": {
    "@paypal/react-paypal-js": "^8.9.2",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@turf/boolean-point-in-polygon": "^7.3.1",
    "@turf/helpers": "^7.3.1",
    "@types/leaflet": "^1.9.12",
    "@types/leaflet-draw": "^1.0.13",
    "@types/leaflet.heat": "^0.2.5",
    "d3-force": "^3.0.0",
    "framer-motion": "^12.24.0",
    "jszip": "^3.10.1",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "leaflet.heat": "^0.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-pdf": "^10.2.0",
    "react-router-dom": "^7.10.1",
    "recharts": "^3.6.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^10.1.2",
    "@storybook/addon-docs": "^10.1.2",
    "@storybook/addon-onboarding": "^10.1.2",
    "@storybook/addon-vitest": "^10.1.2",
    "@storybook/react-vite": "^10.1.2",
    "@storybook/testing-library": "^0.2.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/d3-force": "^3.0.10",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14",
    "autoprefixer": "^10.4.22",
    "chromatic": "^13.3.4",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.1.2",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "storybook": "^10.1.2",
    "tailwindcss": "^3.4.11",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  }
}
